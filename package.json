{
  "name": "ratel",
  "version": "1.0.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "test": "umi test",
    "webpack": "umi webpack",
    "lint": "eslint {src,mock,tests}/**/*.{ts,tsx} --fix",
    "precommit": "lint-staged",
    "start:electron": "cross-env NODE_ENV=development electron-webpack dev",
    "start:electron:prod": "cross-env NODE_ENV=production electron-webpack",
    "build:electron:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build:electron:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "electron:run": "electron .",
    "electron:debug": "electron --inspect=5858 .",
    "electron:dir": "electron-builder -- --dir",
    "electron:win": "electron-builder --win",
    "electron:mac": "electron-builder --macos",
    "svgr": "svgr"
  },
  "gitHooks": {
    "pre-commit-ignore": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "//disable temporary here!  prettier --write"
    ],
    "*.ts?(x)": [
      "//disable temporary here!  prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-card": "^2.3.2",
    "@ant-design/pro-layout": "^7.10.3",
    "@ant-design/pro-table": "^3.6.1",
    "@antv/data-set": "^0.11.5",
    "@antv/g-math": "^1.7.44",
    "@babel/core": "^7.17.5",
    "@babel/preset-typescript": "^7.16.7",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@flatten-js/core": "^1.3.4",
    "@svgdotjs/svg.js": "^3.2.0",
    "@turf/turf": "^6.5.0",
    "@umijs/fabric": "^2.10.2",
    "@umijs/preset-react": "^1.8.30",
    "antd": "^5.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.2",
    "bizcharts": "^4.0.14",
    "braft-editor": "^2.3.9",
    "canvaskit-wasm": "0.39.1",
    "clipboard": "^2.0.11",
    "crypto-js": "^4.0.0",
    "dva": "^2.6.0-beta.6",
    "flv.js": "^1.5.0",
    "js-file-download": "^0.4.12",
    "pixi.js": "^6.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-router-dom": "^5.1.7",
    "react-test-renderer": "^17.0.2",
    "reactjs-popup": "^2.0.6",
    "source-map-support": "^0.5.16",
    "umi": "3.5.21",
    "umi-plugin-icons": "^0.1.1",
    "use-editable": "^2.3.3"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.9.6",
    "@svgr/cli": "^6.5.1",
    "@types/jest": "^23.3.12",
    "@types/node": "^14.18.12",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-test-renderer": "^17.0.1",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^5.0.1",
    "@umijs/preset-ui": "^2.1.14",
    "babel-eslint": "^9.0.0",
    "babel-plugin-import": "^1.13.3",
    "compression-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "electron": "13.1.8",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.0.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-eslint": "^6.0.0",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^8.52.0",
    "eslint-config-alloy": "^3.7.2",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^0.14.3",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.6.2",
    "webpack": "^4.43.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "main.js",
  "electronWebpack": {
    "main": {
      "sourceDirectory": "server"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "build": {
    "productName": "Ratel",
    "appId": "com.ivipi.ratel",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/",
      "main.js"
    ],
    "copyright": "ivipa",
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "static/favicon.ico",
      "uninstallerIcon": "static/favicon.ico",
      "installerHeaderIcon": "static/favicon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Ratel"
    },
    "publish": {
      "provider": "github",
      "repo": "xxxx",
      "owner": "xxxx",
      "token": "xxxxxxxxxxxxxxx",
      "releaseType": "release",
      "publishAutoUpdate": true
    },
    "win": {
      "icon": "static/favicon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "mac": {
      "icon": "static/favicon-512.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "icon": "static/favicon.ico",
      "target": [
        "deb",
        "zip"
      ]
    }
  }
}